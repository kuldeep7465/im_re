@model List<image.Models.filshhd>
<table class="table table-bordered">
    <tr>
        <th>id</th>
        <th>name</th>
        <th>imame</th>
    </tr>
    @foreach (var i in Model)
    {
        <tr>

        <td>@i.id</td>
        <td>@i.name</td>
        <td><img src="@Url.Content(i.imagefile)" width="100" height="100" /></td>
        </tr>
    }
</table>